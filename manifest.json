{
	"manifest_version": 3,
	"name": "Auto Tab Renamer",
	"version": "1.0",
	"description": "Renames tabs that contain specific URL matches.",
	"permissions": [
		"activeTab",
		"tabs",
		"scripting",
		"storage"
	],
	"host_permissions": [
		"*://*/*"
	],
	"background": {
		"service_worker": "background.js"
	},
	"options_page": "options.html",
	"action": {
		"default_popup": "",
		"default_icon": {
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
		}
	},
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
